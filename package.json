{
  "private": true,
  "name": "node-typescript-starter",
  "description": "Template for bootstrap projects with Node.js and TypeScript",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Rafaell Lycan <sonny.webdsg@gmail.com> (https://github.com/rafaell-lycan)",
  "scripts": {
    "build": "rm -rf dist && tsup src",
    "dev": "tsx watch --ignore node_modules src/index.ts",
    "format": "prettier . --write",
    "lint": "eslint src --fix",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nock": "^13.3.0",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "vitest": "^0.27.3"
  }
}
